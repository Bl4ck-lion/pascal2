---
import Layout from "../layouts/Layout.astro";
const backendURL = "../../09%20SchoolworkListMDX-UI";
---
<Layout title="SWL - Viewing Post">
	<main class="m-auto min-h-[85.75vh] max-w-[100%] max-md:max-w-full">
        <div data-service="mdxui" style="width: 100%; height: 90vh;">
            <div data-placeholder data-visible class="flex-col">
                <img class="w-[20%]" src="./404.jpg" />
                <p>An error occoured while loading the data.</p>
            </div>
        </div>
	</main>
</Layout>
<script is:inline>
    var im = iframemanager();
    im.run({
    currLang: 'en',
    services: {
        mdxui: {
            embedUrl: 'https://www.youtube-nocookie.com/embed/dQw4w9WgXcQ',

            thumbnailUrl: 'https://i3.ytimg.com/vi/dQw4w9WgXcQ/hqdefault.jpg',

            iframe: {
                allow: 'encrypted-media; fullscreen;',
            },

            languages: {
                en: {
                    notice: 'This content is hosted by a community. On loading the content, this page will fetch the data from a seperate server that is controlled by the community.',
                    loadBtn: 'Load it once',
                    loadAllBtn: "Load it and Don't ask again"
                }
            }
        }
    }
});
</script>
